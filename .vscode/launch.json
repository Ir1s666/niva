  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable normal example 'tauri-lite'",
      "cargo": {
        "args": ["build", "--bin=tauri-lite", "--package=tauri-lite"],
        "filter": {
          "name": "tauri-lite",
          "kind": "bin"
        },
        "env": {
          "RUST_BACKTRACE": "-l framework=WebKit"
        }
      },
      "args": ["example"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
